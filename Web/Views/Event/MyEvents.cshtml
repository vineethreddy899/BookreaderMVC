@model IList<Web.Models.EventModel>

@{
    ViewData["Title"] = "Index"; 
}


<div class="container">
    @if (Model.Count == 0)
    {
        <div>
            <h4 class="text-info">No Event Created yet</h4>
        </div> 
    }
    else
    {
        <partial name="_DisplayEvents" model="@Model" />
    }

</div>
